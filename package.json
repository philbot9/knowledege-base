    "express": "~4.16.1",
    "helmet": "^4.4.1",
    "http-errors": "^1.6.3",
    "morgan": "~1.9.1",
    "ramda": "^0.27.1",
    "xid": "0.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.42",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/ramda": "^0.27.40",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
